{
  "name": "DeFacto Extension",
  "version": "0.1",
  "description": "Peer-review the internet",
  "permissions": ["tabs", "activeTab", "storage"],
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://*.firebase.com https://www.googleapis.com; object-src 'self'",
  "background": {
    "page":"background.html",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/defacto_extension_16.png",
      "48": "images/defacto_extension_48.png",
      "128": "images/defacto_extension_128.png"
    }
  },
  "icons": {
    "16": "images/defacto_extension_16.png",
    "48": "images/defacto_extension_48.png",
    "128": "images/defacto_extension_128.png"  },
  "manifest_version": 2
}
